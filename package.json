{
    "name": "angular-quickstart",
    "version": "1.0.0",
    "description": "QuickStart package.json from the documentation, supplemented with testing support",
    "scripts": {
        "start-": "tsc && concurrently \"tsc -w\" \"lite-server\" ",
        "e2e": "tsc && concurrently \"http-server -s\" \"protractor protractor.config.js\" --kill-others --success first",
        "lint": "tslint ./app/**/*.ts -t verbose",
        "lite": "lite-server",
        "pree2e": "webdriver-manager update",
        "test": "tsc && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
        "test-once": "tsc && karma start karma.conf.js --single-run",
        "tsc": "tsc",
        "tsc:w": "tsc -w",
        "ngc": "./node_modules/.bin/ngc -p ts-aot.json",
        "build:dev": " rimraf aot/*.* && webpack --config webpack.config.dev.js --colors --progress --watch ",
        "build:aot": " rimraf aot/*.* && webpack --config webpack.config.js --colors --progress --watch ",
        "aot:prod": "./node_modules/.bin/webpack --config webpack.config.js --colors --progress --watch",
        "clean:aot": "npm run rimraf -- aot/*.*",
        "node-sass": "node-sass styles/ -o styles/",
        "start3": "concurrently \"http-server ./aot -o \" \"node-sass -w src/ -o src/ \" ",
        "build:webdev": " rimraf aot/*.* && webpack --config webpack.config.devsrv.js --profile --colors --progress --watch ",
        "server:dev": "webpack-dev-server --config webpack.config.devsrv.js --env.ENV development --progress --profile --content-base aot/ ",
        "server:dev:hmr": "npm run server:dev -- --inline --hot",
        "start": "concurrently \"http-server ./aot -o \" \"node-sass -w src/ -o src/ \" ",
        "clean:debug": "npm run rimraf -- bin/debug/*.*",
        "clean:release": "npm run rimraf -- bin/release/*.*",
        "node-sass:w": "node-sass -w app/ -o app/",
        "config-node": "node --max_old_space_size=4096",
        "build:dev:debug": "webpack --config webpack.config.dev.1.js --watch --colors --progress",
        "start:dev:hmr": "webpack-dev-server --config webpack.config.dev.js --history-api-fallback --port 5001 --hot --client-log-level error --open --watch --colors --progress --compress --content-base bin/debug",
        "build:dll": "rimraf bin/debug/**/*.* && webpack --config webpack.dll.config.js --colors --progress",
        "build:dev:hmr": "rimraf bin/debug/**/*.* && webpack --config webpack.dll.config.js --colors --progress && webpack-dev-server --config webpack.config.dev.js --history-api-fallback --port 9000 --host 0.0.0.0 --hot --client-log-level error --watch --colors --progress --compress --content-base bin/debug",
        "build:prod": "rimraf bin/release/*.* && webpack --config webpack.config.js --colors --progress",
        "hsweb": "http-server ./bin/release -o",
        "hsweb2": " concurrently \"http-server ./dist -o \" \"nodemon server.js\" ",
        "server": "concurrently \" node cluster.js\" \"webpack --config webpack.config.dev.1.js --watch --colors --progress\" "
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@angular/animation": "^4.0.0-beta.8",
        "@angular/animations": "^4.3.6",
        "@angular/common": "^4.3.6",
        "@angular/compiler": "^4.3.6",
        "@angular/compiler-cli": "^4.3.6",
        "@angular/core": "^4.3.6",
        "@angular/forms": "^4.3.6",
        "@angular/http": "^4.3.6",
        "@angular/platform-browser": "^4.3.6",
        "@angular/platform-browser-dynamic": "^4.3.6",
        "@angular/router": "^4.3.6",
        "@types/d3": "^4.3.0",
        "@types/snapsvg": "^0.4.27",
        "compression": "^1.6.2",
        "core-js": "^2.4.1",
        "cors": "^2.8.1",
        "cryptico": "1.0.2",
        "crypto-js": "^3.1.9-1",
        "d3": "^4.4.0",
        "draggabilly": "^2.1.1",
        "express": "^4.14.0",
        "helmet": "^3.1.0",
        "https": "^1.0.0",
        "jsencrypt": "2.3.1",
        "morgan": "^1.7.0",
        "primeng": "^4.0.0",
        "print-js": "^1.0.20",
        "reflect-metadata": "^0.1.10",
        "rxjs": "^5.5.2",
        "snapsvg": "^0.5.1",
        "zone.js": "^0.8.17"
    },
    "devDependencies": {
        "@angular-cli/ast-tools": "^1.0.16",
        "@angular/tsc-wrapped": "^4.3.6",
        "@angularclass/hmr": "^2.1.3",
        "@angularclass/hmr-loader": "^3.0.4",
        "@ngtools/webpack": "^1.6.2",
        "@types/cryptojs": "^3.1.29",
        "@types/node": "^8.0.26",
        "@types/snapsvg": "^0.4.29",
        "angular2-load-children-loader": "^0.2.0",
        "angular2-router-loader": "^0.3.5",
        "angular2-template-loader": "^0.6.2",
        "assets-webpack-plugin": "^3.5.1",
        "awesome-typescript-loader": "^3.2.3",
        "compression-webpack-plugin": "^1.0.0",
        "concurrently": "^3.5.0",
        "copy-webpack-plugin": "^4.0.1",
        "css-loader": "^0.28.7",
        "extract-text-webpack-plugin": "^3.0.0",
        "file-loader": "^0.11.2",
        "html-webpack-plugin": "^2.30.1",
        "json-loader": "^0.5.7",
        "node-sass": "^4.5.3",
        "nodemon": "^1.11.0",
        "raw-loader": "^0.5.1",
        "rimraf": "^2.5.4",
        "sass-loader": "^6.0.6",
        "string-replace-loader": "^1.3.0",
        "style-loader": "^0.18.2",
        "to-string-loader": "^1.1.5",
        "typescript": "^2.4.2",
        "url-loader": "^0.5.9",
        "webpack": "^3.5.5",
        "webpack-dev-server": "^2.7.1",
        "webpack-merge": "^4.1.0"
    },
    "repository": {}
}
